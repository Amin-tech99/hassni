{
  "deploy": {
    "startCommand": "./prestart.sh && gunicorn main:app --bind 0.0.0.0:$PORT --timeout 120 --workers 2 --threads 2 --max-requests 1000 --max-requests-jitter 50 --log-level info",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "healthcheckPath": "/health",
    "healthcheckTimeout": 60
  }
}
